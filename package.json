{
  "name": "leap",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod": "cd ios && arch -x86_64 pod install",
    "reset": "react-native start --reset-cache",
    "clean": "cd android/ && ./gradlew clean",
    "all-m1": "watchman watch-del-all && rm -rf package-lock.json && rm -rf yarn.lock && rm -rf node_modules/ && rm -fr $TMPDIR/metro* && yarn install && cd ios && rm -rf Pods && rm -rf Podfile.lock &&  arch -x86_64 pod install && cd ..",
    "codepushApps": "appcenter apps list",
    "codepushKey": "appcenter codepush deployment list --app limeeted/Leap-ios -k && appcenter codepush deployment list --app limeeted/LImeetED-android -k",
    "depoloyStatus": "appcenter codepush deployment list -a limeeted/LImeetED-android && appcenter codepush deployment list -a limeeted/Leap-ios",
    "depoloyStage": "appcenter codepush release-react -a limeeted/Leap-ios -d Staging && appcenter codepush release-react -a limeeted/LImeetED-android -d Staging",
    "depoloyProd": "appcenter codepush release-react -a limeeted/Leap-ios -d Production && appcenter codepush release-react -a limeeted/LImeetED-android -d Production",
    "depoloyStageAOS": "appcenter codepush release-react -a limeeted/LImeetED-android -d Staging",
    "depoloyStageIOS": "appcenter codepush release-react -a limeeted/Leap-ios -d Staging",
    "depoloyProdAOS": "appcenter codepush release-react -a limeeted/LImeetED-android -d Production",
    "depoloyProdIOS": "appcenter codepush release-react -a limeeted/Leap-ios -d Production",
    "postinstall": "patch-package"
  },
